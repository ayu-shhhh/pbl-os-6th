CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;
    PARTS:
    
    DMux(in=in,sel=sel[2],a=x,b=y);

    DMux(in=x,sel=sel[1],a=i,b=j);
    DMux(in=y,sel=sel[1],a=k,b=l);

    DMux(in=i,sel=sel[0],a=a,b=b);
    DMux(in=j,sel=sel[0],a=c,b=d);
    DMux(in=k,sel=sel[0],a=e,b=f);
    DMux(in=l,sel=sel[0],a=g,b=h);
}

