CHIP RAM4K {
    IN in[16], load, address[12];
    OUT out[16];

    PARTS:
    DMux8Way(in=load,sel=address[9..11],a=l0,b=l1,c=l2,d=l3,e=l4,f=l5,g=l6,h=l7);

    RAM512(in=in,load=l0,address=address[0..8],out=ro0);
    RAM512(in=in,load=l1,address=address[0..8],out=ro1);
    RAM512(in=in,load=l2,address=address[0..8],out=ro2);
    RAM512(in=in,load=l3,address=address[0..8],out=ro3);
    RAM512(in=in,load=l4,address=address[0..8],out=ro4);
    RAM512(in=in,load=l5,address=address[0..8],out=ro5);
    RAM512(in=in,load=l6,address=address[0..8],out=ro6);
    RAM512(in=in,load=l7,address=address[0..8],out=ro7);

    Mux8Way16(a=ro0,b=ro1,c=ro2,d=ro3,e=ro4,f=ro5,g=ro6,h=ro7,sel=address[9..11],out=out);

}
